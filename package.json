{
  "name": "blockchain",
  "version": "0.0.1",
  "description": "",
  "main": "main.ts",
  "scripts": {
    "build:dev": "babel src -w -x '.ts' -d build",
    "app": "node build/main.js",
    "serve": "nodemon build/main.js",
    "start": "run-p build:dev serve",
    "lint": "run-p tscheck tslint",
    "tscheck": "tsc --project ./tsconfig.json --noEmit",
    "tslint": "tslint --config ./tslint.json './src/**/*.ts'",
    "precommit": "lint-staged",
    "coverage": "jest && codecov",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "lint-staged": {
    "*.ts": [
      "tslint --config ./tslint.json './src/**/*.ts' --fix"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "./build/",
      "./node_modules/",
      "./coverage/"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "author": "keyiiiii",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/plugin-syntax-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-es2016": "^7.0.0-beta.46",
    "@babel/preset-es2017": "^7.0.0-beta.46",
    "@babel/preset-typescript": "^7.0.0-beta.45",
    "@types/jest": "^23.3.1",
    "@types/ws": "^5.1.2",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.6.1",
    "codecov": "^3.0.4",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.5",
    "tslint": "^5.10.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "hex-to-binary": "^1.0.1",
    "ws": "^5.2.0"
  }
}
